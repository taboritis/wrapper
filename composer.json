{
    "name": "taboritis/wrapper",
    "description": "A simple way to wrap any data in objects",
    "type": "library",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.7",
        "pestphp/pest": "^2.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Taboritis\\Wrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Taboritis\\Wrapper\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Piotr Skrobol",
            "email": "pio.skro@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
